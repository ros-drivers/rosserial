<?xml version="1.0"?>
<package format="2">
  <name>rosserial_test</name>
  <version>0.9.0</version>
  <description>
    A specialized harness which allows end-to-end integration testing of the
    rosserial client and server components.
  </description>

  <maintainer email="mpurvis@clearpath.ai">Mike Purvis</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>roscpp</depend>
  <depend>gtest</depend>
  <depend>rosserial_client</depend>
  <depend>rosserial_msgs</depend>
  <depend>rosserial_python</depend>
  <depend>rosserial_server</depend>
  <depend>rostest</depend>
  <depend>std_msgs</depend>
 </package>
